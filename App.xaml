<Application x:Class="HowrseBot.App"
			 xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
			 xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
			 xmlns:local="clr-namespace:HowrseBot"
			 StartupUri="StartUp.xaml">
	<Application.Resources>
		
		<ResourceDictionary>
			<ResourceDictionary.MergedDictionaries>
				<ResourceDictionary Source="Resources/ListBoxStyle.xaml"/>
				<ResourceDictionary Source="Simple Styles.xaml"/>
				<ResourceDictionary Source="Resources/MainWindowStyle.xaml"/>
			</ResourceDictionary.MergedDictionaries>
			<BitmapImage x:Key="play" UriSource="Resources/play.png"/>
			<BitmapImage x:Key="pause" UriSource="Resources/pause.png"/>
			<BitmapImage x:Key="delete" UriSource="Resources/delete.png"/>
		</ResourceDictionary>
		
	</Application.Resources>
</Application>
